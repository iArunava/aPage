<h1> All Articles </h1>

<ul id="articles">
    <% @articles.each do |article| %>
        <li>
            <h4><%= link_to article.title, article_path(article) %></h4>
            <p><%= article.body %></p>
        </li>
    <% end %>
</ul>
<%= link_to "Create new Article", new_article_path, class: "new_article" %>
